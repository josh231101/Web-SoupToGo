<div id="carouselexample" class="carousel slide carousel-multi-item" data-ride="carousel">
  <div class="carousel-inner">
    <!--First slide-->
    <div class="carousel-item active">
      <%soups.forEach(function(element){%>

        <div class="col-md-3" style="float:left">
          <div class="card mb-2">
            <img class="card-img-top"
            src="<%= element.imgUrl%>" alt="Card image cap">
            <div class="card-body">
              <h4 class="card-title"><%= element.title %></h4>
              <a class="btn btn-primary">Button</a>
            </div>
          </div>
        </div>
      <%})%>

    </div>
